<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="San\UserBundle\Entity\User" table="san_user">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <field name="cell" type="bigint" nullable="true" />
        <field name="firstName" type="string" nullable="true" />
        <field name="lastName" type="string" nullable="true" />
        <field name="registered" type="datetime" />
    </entity>

</doctrine-mapping>
