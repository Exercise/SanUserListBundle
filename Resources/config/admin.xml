<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="san.admin.user" class="San\UserListBundle\Admin\UserAdmin">
            <tag name="sonata.admin" manager_type="" group="User" label="User"/>
            <tag name="san.admin" />
            <argument />
            <argument>%san_user_list.user_class%</argument>
            <argument></argument>
            <call method="setTranslationDomain">
                <argument>SanUserBundle</argument>
            </call>
        </service>

        <service id="san.admin.user_static_list" class="San\UserListBundle\Admin\UserStaticListAdmin">
            <tag name="sonata.admin" manager_type="" group="User" label="User Static List"/>
            <tag name="san.admin" />
            <argument />
            <argument>San\UserListBundle\Entity\UserStaticList</argument>
            <argument></argument>
            <call method="setTranslationDomain">
                <argument>SanUserBundle</argument>
            </call>
        </service>

        <service id="san.admin.user_dynamic_list" class="San\UserListBundle\Admin\UserDynamicListAdmin">
            <tag name="sonata.admin" manager_type="" group="User" label="User Dynamic List"/>
            <tag name="san.admin" />
            <argument />
            <argument>San\UserListBundle\Entity\UserDynamicList</argument>
            <argument>SanUserListBundle:Admin/UserDynamicListCRUD</argument>
            <call method="setTranslationDomain">
                <argument>SanUserBundle</argument>
            </call>
        </service>

    </services>
</container>
