<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="san.admin.user" class="San\UserBundle\Admin\UserAdmin">
            <tag name="sonata.admin" manager_type="" group="User" label="User"/>
            <argument />
            <argument>San\UserBundle\Document\User</argument>
            <argument />
            <call method="setTranslationDomain">
                <argument>SanUserBundle</argument>
            </call>
            <call method="setManager">
                <argument>%san_user.manager%</argument>
            </call>
        </service>
    </services>
</container>
